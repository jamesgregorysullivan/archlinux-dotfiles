[Unit]
Description=Chromium web browser
After=network.target

[Service]
UMask=0077
Environment=DISPLAY=:0
EnvironmentFile=/etc/chromium/default
ExecStart=/usr/lib/chromium/chromium $CHROMIUM_FLAGS --disk-cache-dir=/tmp/lahwaacz-chromium-cache --disk-cache-size=104857600 --log-level=2
KillSignal=SIGINT

[Install]
WantedBy=graphical.target
